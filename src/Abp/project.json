{
  "version": "0.9.6.0-*",

  "dependencies": {
    "Castle.LoggingFacility": "3.3.0",
    "Castle.Windsor": "3.3.0",
    "Newtonsoft.Json": "8.0.3",
    "Nito.AsyncEx": "3.0.1",
    "System.Collections.Immutable": "1.1.36"
  },

  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Configuration": "",
        "System.Data": "",
        "System.Transactions": "",
        "System.Runtime": {
          "type": "build"
        },
        "System.Runtime.Caching": "",
        "System.ComponentModel.DataAnnotations": "",
        "System.Threading.Tasks": {
          "type": "build"
        }
      }
    }
  },

  "buildOptions": {
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "embed": {
      "include": [
        "Localization/Sources/AbpXmlSource/*.xml",
        "Timing/Timezone/Mapping/*.xml"
      ]
    }
  },

  "packOptions": {
    "summary": "ASP.NET Boilerplate is a general-purpose, web focused application framework.",
    "tags": [ "asp.net", "asp.net mvc", "boilerplate", "application framework", "web framework", "framework", "domain driven design" ],
    "owners": [ "Halil İbrahim Kalkan" ],
    "iconUrl": "http://www.aspnetboilerplate.com/images/abp_nupkg.png",
    "projectUrl": "http://www.aspnetboilerplate.com/",
    "licenseUrl": "https://github.com/aspnetboilerplate/aspnetboilerplate/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnetboilerplate/aspnetboilerplate"
    }
  },

  "packInclude": {
    "lib/net452/": "bin/Release/net452/Abp.pdb"
  }
}
