{
  "version": "0.9.1.2-*",

  "dependencies": {
    "Abp": "0.9.1.2-*"
  },

  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0"
      }
    }
  },

  "buildOptions": {
    "xmlDoc": true,
    "embed": {
      "include": [
        "Web/Localization/AbpWebXmlSource/*.xml"
      ]
    }
  },

  "packOptions": {
    "summary": "System.Web integration package for ASP.NET Boilerplate.",
    "tags": [ "asp.net", "asp.net mvc", "boilerplate", "application framework", "web framework", "framework", "domain driven design", "web" ],
    "owners": [ "Halil İbrahim Kalkan" ],
    "iconUrl": "http://www.aspnetboilerplate.com/images/abp_nupkg.png",
    "projectUrl": "http://www.aspnetboilerplate.com/",
    "licenseUrl": "https://github.com/aspnetboilerplate/aspnetboilerplate/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnetboilerplate/aspnetboilerplate"
    }
  },

  "packInclude": {
    "lib/net452/": "bin/Release/net452/Abp.Web.pdb"
  }
}
